             {t('work')}\r\n                    </Link>\r\n                  }\r\n                </Grid>\r\n                <Grid item xs={1} style={{marginTop:'1.5rem', marginLeft:'-1rem'}}>\r\n                  {mode==='day'&&\r\n                    <Link \r\n                      href=\"./Writings\"\r\n                      underline='none'\r\n                      className={router.pathname == '/Writings'?classes.linkAnchor:classes.link}\r\n                    >\r\n                      {t('writings')}\r\n                    </Link>\r\n                  }\r\n                  {mode==='night'&&\r\n                    <Link \r\n                      href=\"./Writings\"\r\n                      underline='none'\r\n                      className={router.pathname == '/Writings'?classes.nightLinkAnchor:classes.nightLink}\r\n                    >\r\n                      {t('writings')}\r\n                    </Link>\r\n                  }\r\n                </Grid>\r\n                <Grid item xs={1} style={{marginTop:'1.5rem', marginLeft:'0.5rem'}}>\r\n                  {mode==='day'&&\r\n                    <Link \r\n                      href=\"./About\"\r\n                      underline='none'\r\n                      className={router.pathname == '/About'?classes.linkAnchor:classes.link}\r\n                    >\r\n                      {t('about')}\r\n                    </Link>\r\n                  }\r\n                  {mode==='night'&&\r\n                    <Link \r\n                      href=\"./About\"\r\n                      underline='none'\r\n                      className={router.pathname == '/About'?classes.nightLinkAnchor:classes.nightLink}\r\n                    >\r\n                      {t('about')}\r\n                    </Link>\r\n                  }\r\n                </Grid>\r\n                <Grid item xs={1} style={{marginTop:'1.5rem', paddingLeft:'0.5rem'}}>\r\n                  {/* <Link \r\n                    href=\"./Resume\"\r