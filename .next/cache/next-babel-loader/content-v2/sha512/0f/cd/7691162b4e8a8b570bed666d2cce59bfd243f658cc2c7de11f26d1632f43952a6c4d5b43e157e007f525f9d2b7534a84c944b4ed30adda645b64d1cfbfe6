andleBack() {\r\n        if(!this.props.rotateBy) {\r\n            if(this.state.count > 3) {\r\n                this.setState((prevState)=>({\r\n                    count: prevState.count - 3\r\n                }));\r\n            }else {\r\n                this.setState({count: 0});\r\n            }\r\n        }else {\r\n            if(this.state.count > 3) {\r\n                this.setState((prevState, props)=>({\r\n                    count: prevState.count - props.rotateBy\r\n                }));\r\n            }else {\r\n                this.setState({count: 0});\r\n            }\r\n            \r\n        }\r\n    }\r\n\r\n    render() {\r\n        \r\n        const {\r\n            imgsrc,\r\n            rotateBy,\r\n            width,\r\n            height,\r\n            controlButton,\r\n            navigateButton\r\n        } = this.props;\r\n\r\n        const list = [];\r\n\r\n        if(!imgsrc) {\r\n            return list\r\n        }else {\r\n            if(!rotateBy) {\r\n                for(let i = 0; i <= imgsrc.length - 1; i++) {\r\n                    list.push(\r\n                        <div key={i}>\r\n                            {parseInt( this.state.count / 3) % imgsrc.length === i ?\r\n                                <div style={{width:'100%'}}>\r\n                                    <img src={imgsrc[i]} style={{margin: '0 auto', width:'100%', height:'100%'}}/>\r\n                                </div>\r\n                            :''}        \r\n                        </div>\r\n                    )\r\n                }\r\n            }else {\r\n                for(let i = 0; i <= imgsrc.length - 1; i++) {\r\n                    list.push(\r\n                        <div key={i}>\r\n                            {parseInt( this.state.count / rotateBy) % imgsrc.length === i ?\r\n                                <div style={{width:'100%'}}>\r\n                                    <img src={imgsrc[i]} style={{margin: '0 auto', width:'100%', height:'100%'}}/>\r\n                                </div>\r\n                            :''}        \r\n                        </div>\r\n                    )\r\n                }\r\n            }\r\n        }\r\n    \r\n        const RadioNavigation = [];\r\n\r\n        if(!imgsrc) {\r\n            return RadioNavigation\r\n        }else {\r\n            if(!rotateBy) {\r\n                for(let i = 0; i <= imgsrc.length - 1; i++) {\r\n                    RadioNavigation.push(\r\n                        <div>\r\n                            {parseInt( this.state.count / 3) % imgsrc.length === i ?\r\n                                <IconButton onClick={()=>{this.setState({count: 3 * i})}}>\r\n                                    <PlayCircleFilledIcon/>\r\n                                </IconButton>\r\n                            :\r\n                                <IconButton onClick={()=>{this.setState({count: 3 * i})}}>\r\n                                    <RadioButtonUncheckedIcon/>\r\n                                </IconButton>\r\n                            }\r\n                        </div>\r\n                    )\r\n                }\r\n            }else {\r\n                for(let i = 0; i <= imgsrc.length - 1; i++) {\r\n                    RadioNavigation.push(\r\n                        <div>\r\n                            {parseInt( this.state.count / rotateBy) % imgsrc.length === i ?\r\n                                <IconButton onClick={()=>{this.setState({count: rotateBy * i})}}>\r\n                                    <PlayCircleFilledIcon/>\r\n                                </IconButton>\r\n                            :\r\n                                <IconButton onClick={()=>{this.setState({count: rotateBy * i})}}>\r\n                                    <RadioButtonUncheckedIcon/>\r\n                                </IconButton>\r\n                            }\r\n                        </div>\r\n                    )\r\n                }\r\n            }\r\n        }\r\n\r\n        return(\r\n            <React.Fragment>\r\n\r\n                <Grid container justify=\"center\" alignItems=\"center\">\r\n                \r\n                    <Grid item xs={12}>\r\n                        {list}\r\n                    </Grid>\r\n\r\n                \r\n                </Grid>\r\n\r\n                <Grid container justify=\"center\" alignItems=\"center\">\r\n                    <Grid item xs={2} style={{textAlign:'left'}}>\r\n                        {controlButton === true &&\r\n                            <Button style={{margin:'auto 0'}} onClick={this.handleBack}>\r\n                                <ArrowBackIosIcon/>\r\n                            </Button>\r\n                        }\r\n                    </Grid>\r\n                    <Grid item xs={8}>\r\n                        {/* {navigateButton === true ?\r\n                            {RadioNavigation}\r\n                        :''} */}\r\n                    </Grid>\r\n                    <Grid item xs={2} style={{textAlign:'right'}}>\r\n                        {controlButton === true &&\r\n                            <Button style={{margin:'auto 0'}} onClick={this.handleNext}>\r\n                                <ArrowForwardIosIcon/>\r\n                            </Button>\r\n                        }\r\n                    </Grid>\r\n                </Grid>\r\n                \r\n\r\n            </React.Fragment>\r\n        )\r\n    }\r\n}\r\n\r\nexport default CustomizeCarousel;"]},"metadata":{},"sourceType":"module"}